  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run godot",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "C:/Users/f/Documents/Godot/Godot.exe",
      "cwd": "${workspaceFolder}",
      "console": "externalTerminal",
      "externalConsole": true,
      "stopAtEntry": false,
      "args": ["--path", "${workspaceRoot}"]
    }
  ]
}
